{
  "name": "facebook-uid-finder",
  "version": "1.0.0",
  "description": "Facebook UID Finder web interface",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run build-api",
    "build-api": "mkdir -p dist/api && cp api/* dist/api/",
    "preview": "vite preview",
    "serve": "vite preview --port 4173",
    "build:analyze": "vite build --mode analyze",
    "clean": "rm -rf dist",
    "build:clean": "npm run clean && npm run build"
  },
  "dependencies": {
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "terser": "^5.41.0",
    "vite": "^5.0.0"
  },
  "keywords": [
    "facebook",
    "uid",
    "finder",
    "javascript",
    "vite"
  ],
  "author": "Quoc",
  "license": "MIT"
}
